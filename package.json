{
  "name": "chayns-helper",
  "version": "2.13.10-beta.2",
  "author": {
    "name": "Christian Nagel",
    "email": "christian.nagel@tobit.com"
  },
  "description": "my personal helpers for chayns development",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "module": "dist/chayns-helper.esm.js",
  "license": "MIT",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "test": "tsdx test",
    "test:coverage": "tsdx test --coverage",
    "build": "tsdx build",
    "install:all": "npm install --ignore-scripts && npm install chayns-logger -reg http://repo.tobit.ag:8081/repository/npm/ --no-save",
    "pub:beta": "npm version prerelease --preid=beta --no-git-tag-version && npm publish && node scripts.js -d \"%name%@%version% is unstable, please install a stable version\"",
    "pub:patch": "npm version patch --no-git-tag-version && npm publish",
    "pub:minor": "npm version minor --no-git-tag-version && npm publish",
    "pub:major": "npm version major --no-git-tag-version && npm publish",
    "git:postpublish": "node scripts.js -pushtags",
    "lint": "tsdx lint src",
    "size": "size-limit",
    "size:analyze": "size-limit --why",
    "prepare": "npm run build",
    "preversion": "npm run install:all && npm run test:coverage && npm run size",
    "postpublish": "node scripts.js -commit"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "peerDependencies": {
    "chayns-components": ">=4.20.4",
    "chayns-logger": ">=2.8.5",
    "react": ">=16",
    "react-redux": ">=7.1.3"
  },
  "peerDependenciesMeta": {
    "react-redux": {
      "optional": true
    },
    "react": {
      "optional": false
    },
    "chayns-logger": {
      "optional": true
    },
    "chayns-components": {
      "optional": false
    }
  },
  "size-limit": [
    {
      "path": "dist/chayns-helper.cjs.production.min.js",
      "limit": "65KB"
    },
    {
      "path": "dist/chayns-helper.esm.js",
      "limit": "50KB"
    },
    {
      "path": "dist/chayns-helper.esm.js",
      "import": "{ statusAnimations }",
      "limit": "25.45KB"
    }
  ],
  "prettier": {
    "tabWidth": 4,
    "printWidth": 120,
    "semi": true,
    "singleQuote": true,
    "jsxSingleQuote": false,
    "trailingComma": "none",
    "arrowParens": "always"
  },
  "devDependencies": {
    "@babel/plugin-proposal-logical-assignment-operators": "^7.8.3",
    "@babel/plugin-proposal-numeric-separator": "^7.8.3",
    "@babel/plugin-proposal-pipeline-operator": "^7.8.3",
    "@babel/plugin-proposal-throw-expressions": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@size-limit/preset-big-lib": "^4.9.2",
    "@types/react-dom": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "autoprefixer": "^9.7.4",
    "babel-plugin-const-enum": "^1.0.1",
    "babel-plugin-optimize-clsx": "^2.6.1",
    "babel-plugin-transform-remove-imports": "^1.5.2",
    "babel-plugin-transform-rename-import": "^2.3.0",
    "babel-plugin-typescript-iife-enum": "^0.2.1",
    "chayns-components": "^4.20.17",
    "chayns-doc": "^1.2.1",
    "cssnano": "^4.1.10",
    "eslint": "^7.18.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.0.4",
    "identity-obj-proxy": "^3.0.0",
    "jest-fetch-mock": "^3.0.3",
    "jsdoc": "^3.6.6",
    "node-sass": "^4.11.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-redux": "^7.1.3",
    "rollup-plugin-postcss": "^3.1.4",
    "rollup-plugin-scss": "^2.6.1",
    "size-limit": "^4.9.1",
    "tsc": "^1.20150623.0",
    "tsdx": "^0.14.1",
    "tslib": "^2.1.0",
    "typedoc": "^0.20.19",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "@babel/runtime": "^7.12.5",
    "@types/node": "^14.14.22",
    "@types/react": "^17.0.0",
    "abortcontroller-polyfill": "^1.7.1",
    "clsx": "^1.1.1",
    "date-fns": "^2.16.1",
    "html-react-parser": "^1.2.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chincoe/chayns-helper.git"
  },
  "keywords": [
    "chayns",
    "react",
    "hooks"
  ],
  "bugs": {
    "url": "https://github.com/chincoe/chayns-helper/issues"
  },
  "homepage": "https://github.com/chincoe/chayns-helper#readme"
}
